props.history.push('/login');\r\n            }\r\n        });\r\n    }\r\n\r\n    render() {\r\n\r\n        return (\r\n            <div id=\"App\">\r\n                {this.props.user && this.props.userData ? <MainPage logout={this.logout} user={this.props.user} userData={this.props.userData[0]}/> : \"Loading user settings...\"}\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nApp.propTypes = {\r\n\r\n};\r\n\r\nexport default withTracker(\r\n    () => {\r\n        Meteor.subscribe('users');\r\n        Meteor.subscribe('UserData');\r\n\r\n        const currentUser = Meteor.user();\r\n        let userData = null;\r\n\r\n        if(currentUser != null){\r\n            //console.log(\"Searching for data of \" + currentUser._id);\r\n            userData = UserData.find(\r\n                { userId: currentUser._id }\r\n            ).fetch();\r\n        }\r\n\r\n        return {\r\n            user: currentUser,\r\n            userData: userData\r\n        }\r\n    }\r\n)(App);","import React from 'react'\r\nimport { BrowserRouter as Router, Route } from 'react-router-dom';\r\n\r\n// Bases\r\nimport App from './App.jsx'\r\n\r\n// Pages\r\nimport Login from '../Accounts/Login.jsx'\r\nimport Signup from '../Accounts/Signup.jsx'\r\n\r\nexport const renderRoutes = () => (\r\n  <Router>\r\n    <div id=\"Routes\">\r\n      <Route exact={true} path=\"/\" component={App}/>\r\n\r\n      <Route path=\"/login\" component={Login}/>\r\n      <Route path=\"/signup\" component={Signup}/>\r\n    </div>\r\n  </Router>\r\n);"]}}]